<isdecorate template="common/layout/page">
    <div class="container">
        <h1>Basket Products</h1>
        <div class="row">
            <isloop items="${pdict.productList}" var="product" status="loopstatus">
                <div class="col-sm-3">
                    <isinclude url="${URLUtils.url('Tile-Show', 'pid', product.productID, 'swatches', true, 'ratings', true)}" />
                </div>
            </isloop>
        </div>
    </div>
</isdecorate>